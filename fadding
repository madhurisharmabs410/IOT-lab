int ledPin=9;
void setup() {
  // put your setup code here, to run once:

}

void loop() {
  // put your main code here, to run repeatedly:
  for(int fadevalue=0;fadevalue<=255;fadevalue+=5)
  {
    analogWrite(ledPin,fadevalue);
    delay(3000);
  }
  for(int fadevalue=255;fadevalue>0;fadevalue+=5)
  {
    analogWrite(ledPin,fadevalue);
    delay(3000);
  }

}
